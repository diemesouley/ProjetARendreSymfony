{% extends 'base.html.twig' %}

{% block title %}Partenaire index{% endblock %}

{% block body %}
    <h1>Partenaire index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Matricule</th>
                <th>NomPartenaire</th>
                <th>Ninea</th>
                <th>EmailPartenaire</th>
                <th>AdressePartenaire</th>
                <th>TelephonePartenaire</th>
                <th>Status</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for partenaire in partenaires %}
            <tr>
                <td>{{ partenaire.id }}</td>
                <td>{{ partenaire.matricule }}</td>
                <td>{{ partenaire.nomPartenaire }}</td>
                <td>{{ partenaire.ninea }}</td>
                <td>{{ partenaire.emailPartenaire }}</td>
                <td>{{ partenaire.adressePartenaire }}</td>
                <td>{{ partenaire.telephonePartenaire }}</td>
                <td>{{ partenaire.status }}</td>
                <td>
                    <a href="{{ path('partenaire_show', {'id': partenaire.id}) }}">show</a>
                    <a href="{{ path('partenaire_edit', {'id': partenaire.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="9">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('partenaire_new') }}">Create new</a>
{% endblock %}
